{
    "manifest_version": 3,
    "name": "Grind Wallet",
    "description": "A Crypto Wallet for the Internet Computer (ICP) blockchain.",
    "version": "0.0.0",
    "permissions": ["storage"],
    "action": {
        "default_popup": "chrome-extension/popup/popup.html",
        "default_icon": {
            "16": "assets/icon16.png",
            "48": "assets/icon48.png",
            "128": "assets/icon128.png"
        }
    },
    "icons": {
        "16": "assets/icon16.png",
        "48": "assets/icon48.png",
        "128": "assets/icon128.png"
    }
}
